{
  "default": "
  <main class='verticalCenter'>
  <p class='singleton'>{sentence}</p>
{input_method}
</main>
",

"loader": "
<p id='loading'>{loading_text}</p>
<div class='spinner'>
<div class='bounce1'></div>
<div class='bounce2'></div>
<div class='bounce3'></div>
</div>
",

"simple": "
<h1>{title}</h1>
<p>{text}</p>
<form class='marginBottom'>{input}</form>
",

"centeredTitle":"
<div class='verticalCenter'>
<h1>{title}</h1>
<form>{input}</form>
</div>
",

"demographic":"
<h1>Done</h1>
<p>{some_text}</p>
<form class='demographic'>
<div class='input' data-run='demographic'>
<table>
<tr class='textbox'>
<td>
<p>Type in your age:</p>
<div><input id='age' type='text' autofocus></div>
</td>
</tr>
<tr class='textbox'>
<td>
<p>Type in your maternal language(s):</p>
<div><input id='lang' type='text'></div>
</td>
</tr>
<tr class='checkbox'>
<td>
<p>Select your biological sex:</p>
<input type='radio' name='gender' id='gender_f' value='f'><label for='gender_f'><span>female</span></label><br/>
<input type='radio' name='gender' id='gender_m' value='m'><label for='gender_m'><span>male</span></label>
</td>
</tr>
<tr class='checkbox'>
<td>
<p>Select your handedness:</p>
<input type='radio' name='hand' id='hand_l' value='left'><label for='hand_l'><span>Left handed</span></label><br/>
<input type='radio' name='hand' id='hand_r' value='right'><label for='hand_r'><span>Right handed</span></label>
</td>
</tr>
<tr class='checkbox'>
<td>
<p>What device(s) did you use to click during this experiment?</p>
<input type='checkbox' name='clicker' id='clicker_trackpad' value='trackpad'><label for='clicker_trackpad'><span>Track-pad</span></label><br/>
<input type='checkbox' name='clicker' id='clicker_touchscreen' value='touchscreen'><label for='clicker_touchscreen'><span>Touch-screen</span></label><br/>
<input type='checkbox' name='clicker' id='clicker_mouse' value='mouse'><label for='clicker_mouse'><span>Computer mouse</span></label><br/>
<input type='checkbox' name='clicker' id='clicker_other' value='other'><label for='clicker_other'><span>Other</span></label><br/>
</td>
</tr>
<tr class='button'>
<td>
<button><span>{button_text}</span></button>
</td>
</tr>
</table>
</div>
</form>
",

"experiments":"
<div data-run=\"experimental_design\" class=\"invisible\" data-practice=\"{is_practice}\">
<div class=\"exp_des_button\" id=\"false\">false</div>
<div class=\"exp_des_button\" id=\"true\">true</div>

<p id=\"sentence\">Error</p>
<div class=\"exp_des_button\" id=\"start\">start</div>
<canvas id=\"debug\"></canvas>
</div>
<div id=\"warning\"><p>Too slow. Please answer faster.</p></div>
",

"calibration":"
<div data-run=\"calibration_run\" class=\"invisible\" >
<div class=\"exp_des_button_2\" id=\"false\">blue</div>
<div class=\"exp_des_button_2\" id=\"true\">red</div>
<div class=\"exp_des_button\" id=\"start\">start</div>
<canvas id=\"points\"></canvas>
</div>

"


}